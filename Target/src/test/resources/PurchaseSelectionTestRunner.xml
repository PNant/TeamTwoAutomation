<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

 <suite name="PurchaseSelectionTestRunner" parallel="false">

     <parameter name="useCloudEnv" value="false"/>
     <parameter name="cloudEnvName" value="browserstack"/>
     <parameter name="os" value="Windows"/>
     <parameter name="os_version" value="10"/>
     <parameter name="browserName" value="chrome"/>
     <parameter name="browserVersion" value="66"/>
     <parameter name="url" value="https://www.target.com/"/>

     <test name = "testPurchaseSelection">
         <classes>
             <class name="testForEyes.testPurchaseSelection"/>
         </classes>
     </test>

 </suite>
